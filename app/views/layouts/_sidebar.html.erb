<table class="table border">
  <thead class="table-dark">
    <tr>
      <th>
        ジャンル検索
      <th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <th>
        <ul class="list-group list-group-flush">
          <% genres.each do |genre| %>
            <% if genre.items.count != 0 %>
              <li class="list-group-item">
                <%= link_to genre.name, genre_path(genre) %>
              </li>
            <% end %>
          <% end %>
        </ul>
      <th>
    </tr>
  </tbody>
</table>